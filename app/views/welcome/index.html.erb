<% if current_user %>
  <P>You're Logged In</p>
  <p><%= button_to "Log Out", logout_path, method: :delete %></p>
  <h3>Existing Users</h3>
  <% @users.each do |user| %>
    <p><div id="user-<%= user.id %>"><%= user.email %></div></p>
  <% end %>
<% else %>
  <p><%= button_to "Create New User", register_path, method: :get %></p>
  <p><%= button_to "Log In", login_path, method: :get %></p>
<% end %>
